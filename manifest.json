{
	"manifest_version": 2,
	"name": "notifyMe",
	"description": "test communication between front-end and back-end",
	"version": "1.1",
	
	"background":  {
		"scripts": ["background.js"] 
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Notifier",
		"default_popup": "index.html"
    },

    "permissions": [
      "notifications",
      "alarms",
      "tabs"
    ]
}